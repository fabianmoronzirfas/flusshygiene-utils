{
  "name": "@tsb/flusshygiene-utils",
  "version": "0.3.1",
  "description": "shared code for the flusshygiene project at tsb",
  "main": "dist/index.js",
  "scripts": {
    "pack": "pack",
    "test": "jest",
    "testWithCoverage": "jest --coverage",
    "build": "tsc -p ."
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg"
      ],
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "__tests__/*"
          ]
        }
      ],
      ["@pika/plugin-build-node"],
      ["@pika/plugin-build-web"]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build"
    }
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@pika/pack": "^0.3.4",
    "@pika/plugin-standard-pkg": "^0.3.13",
    "@pika/plugin-ts-standard-pkg": "^0.3.13",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.9.5",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.13.0",
    "typescript": "^3.3.3333"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technologiestiftung/flusshygiene-utils.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/technologiestiftung/flusshygiene-utils/issues"
  },
  "homepage": "https://github.com/technologiestiftung/flusshygiene-utils#readme"
}
